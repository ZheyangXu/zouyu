<?xml version="1.0" ?>
<mujoco model="Go1 Quadruped Robot">
  <!-- Compiler settings -->
  <compiler angle="radian" meshdir="meshes/" texturedir="textures/"/>
  
  <!-- Size settings -->
  <size nconmax="100" njmax="1000" nstack="300000"/>
  
  <!-- Options -->
  <option gravity="0 0 -9.81" timestep="0.002" viscosity="0" density="0"/>
  
  <!-- Visual settings -->
  <visual>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="2048" offsamples="8"/>
    <map force="0.1" fogstart="3" fogend="10" zfar="40"/>
  </visual>
  
  <!-- Assets -->
  <asset>
    <!-- Basic materials -->
    <material name="blue" rgba="0.2 0.2 0.8 1"/>
    <material name="red" rgba="0.8 0.2 0.2 1"/>
    <material name="green" rgba="0.2 0.8 0.2 1"/>
    <material name="black" rgba="0.2 0.2 0.2 1"/>
    <material name="white" rgba="0.9 0.9 0.9 1"/>
    
    <!-- Ground texture -->
    <texture name="groundplane" type="2d" builtin="checker" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" width="100" height="100"/>
    <material name="groundplane" texture="groundplane" texrepeat="5 5" reflectance="0.2"/>
  </asset>
  
  <!-- World body -->
  <worldbody>
    <!-- Ground plane -->
    <geom name="ground" type="plane" size="40 40 0.1" material="groundplane"/>
    
    <!-- Lighting -->
    <light name="spotlight" mode="targetbodycom" target="torso" pos="0 -1 2"/>
    
    <!-- Go1 Robot -->
    <body name="torso" pos="0 0 0.32">
      <!-- Main body -->
      <geom name="torso" type="box" size="0.267 0.194 0.114" rgba="0.6 0.6 0.6 1" mass="4.713"/>
      <inertial pos="0 0 0" mass="4.713" diaginertia="0.07335 0.25068 0.25447"/>
      
      <!-- IMU sensor -->
      <site name="imu" pos="0 0 0" size="0.01"/>
      
      <!-- Front Right Hip -->
      <body name="FR_hip" pos="0.183 -0.047 0">
        <joint name="FR_hip_joint" type="hinge" axis="1 0 0" range="-0.873 0.873" damping="0.1"/>
        <geom name="FR_hip" type="box" size="0.041 0.020 0.041" rgba="0.8 0.3 0.3 1" mass="0.696"/>
        <inertial pos="0 0 0" mass="0.696" diaginertia="0.000469246 0.000807786 0.000552929"/>
        
        <!-- Front Right Thigh -->
        <body name="FR_thigh" pos="0 -0.083 0">
          <joint name="FR_thigh_joint" type="hinge" axis="0 1 0" range="-2.966 4.5" damping="0.1"/>
          <geom name="FR_thigh" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.041" rgba="0.8 0.3 0.3 1" mass="1.013"/>
          <inertial pos="0 0 -0.125" mass="1.013" diaginertia="0.005529065 0.005529065 0.000681723"/>
          
          <!-- Front Right Calf -->
          <body name="FR_calf" pos="0 0 -0.25">
            <joint name="FR_calf_joint" type="hinge" axis="0 1 0" range="-2.77507 -0.645772" damping="0.1"/>
            <geom name="FR_calf" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.02" rgba="0.8 0.3 0.3 1" mass="0.226"/>
            <inertial pos="0 0 -0.125" mass="0.226" diaginertia="0.001367788 0.001367788 0.000018050"/>
            
            <!-- Front Right Foot -->
            <body name="FR_foot" pos="0 0 -0.25">
              <geom name="FR_foot" type="sphere" size="0.02" rgba="0.5 0.5 0.5 1" mass="0.06"/>
              <inertial pos="0 0 0" mass="0.06" diaginertia="0.000024 0.000024 0.000024"/>
            </body>
          </body>
        </body>
      </body>
      
      <!-- Front Left Hip -->
      <body name="FL_hip" pos="0.183 0.047 0">
        <joint name="FL_hip_joint" type="hinge" axis="1 0 0" range="-0.873 0.873" damping="0.1"/>
        <geom name="FL_hip" type="box" size="0.041 0.020 0.041" rgba="0.3 0.8 0.3 1" mass="0.696"/>
        <inertial pos="0 0 0" mass="0.696" diaginertia="0.000469246 0.000807786 0.000552929"/>
        
        <!-- Front Left Thigh -->
        <body name="FL_thigh" pos="0 0.083 0">
          <joint name="FL_thigh_joint" type="hinge" axis="0 1 0" range="-2.966 4.5" damping="0.1"/>
          <geom name="FL_thigh" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.041" rgba="0.3 0.8 0.3 1" mass="1.013"/>
          <inertial pos="0 0 -0.125" mass="1.013" diaginertia="0.005529065 0.005529065 0.000681723"/>
          
          <!-- Front Left Calf -->
          <body name="FL_calf" pos="0 0 -0.25">
            <joint name="FL_calf_joint" type="hinge" axis="0 1 0" range="-2.77507 -0.645772" damping="0.1"/>
            <geom name="FL_calf" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.02" rgba="0.3 0.8 0.3 1" mass="0.226"/>
            <inertial pos="0 0 -0.125" mass="0.226" diaginertia="0.001367788 0.001367788 0.000018050"/>
            
            <!-- Front Left Foot -->
            <body name="FL_foot" pos="0 0 -0.25">
              <geom name="FL_foot" type="sphere" size="0.02" rgba="0.5 0.5 0.5 1" mass="0.06"/>
              <inertial pos="0 0 0" mass="0.06" diaginertia="0.000024 0.000024 0.000024"/>
            </body>
          </body>
        </body>
      </body>
      
      <!-- Rear Right Hip -->
      <body name="RR_hip" pos="-0.183 -0.047 0">
        <joint name="RR_hip_joint" type="hinge" axis="1 0 0" range="-0.873 0.873" damping="0.1"/>
        <geom name="RR_hip" type="box" size="0.041 0.020 0.041" rgba="0.3 0.3 0.8 1" mass="0.696"/>
        <inertial pos="0 0 0" mass="0.696" diaginertia="0.000469246 0.000807786 0.000552929"/>
        
        <!-- Rear Right Thigh -->
        <body name="RR_thigh" pos="0 -0.083 0">
          <joint name="RR_thigh_joint" type="hinge" axis="0 1 0" range="-2.966 4.5" damping="0.1"/>
          <geom name="RR_thigh" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.041" rgba="0.3 0.3 0.8 1" mass="1.013"/>
          <inertial pos="0 0 -0.125" mass="1.013" diaginertia="0.005529065 0.005529065 0.000681723"/>
          
          <!-- Rear Right Calf -->
          <body name="RR_calf" pos="0 0 -0.25">
            <joint name="RR_calf_joint" type="hinge" axis="0 1 0" range="-2.77507 -0.645772" damping="0.1"/>
            <geom name="RR_calf" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.02" rgba="0.3 0.3 0.8 1" mass="0.226"/>
            <inertial pos="0 0 -0.125" mass="0.226" diaginertia="0.001367788 0.001367788 0.000018050"/>
            
            <!-- Rear Right Foot -->
            <body name="RR_foot" pos="0 0 -0.25">
              <geom name="RR_foot" type="sphere" size="0.02" rgba="0.5 0.5 0.5 1" mass="0.06"/>
              <inertial pos="0 0 0" mass="0.06" diaginertia="0.000024 0.000024 0.000024"/>
            </body>
          </body>
        </body>
      </body>
      
      <!-- Rear Left Hip -->
      <body name="RL_hip" pos="-0.183 0.047 0">
        <joint name="RL_hip_joint" type="hinge" axis="1 0 0" range="-0.873 0.873" damping="0.1"/>
        <geom name="RL_hip" type="box" size="0.041 0.020 0.041" rgba="0.8 0.8 0.3 1" mass="0.696"/>
        <inertial pos="0 0 0" mass="0.696" diaginertia="0.000469246 0.000807786 0.000552929"/>
        
        <!-- Rear Left Thigh -->
        <body name="RL_thigh" pos="0 0.083 0">
          <joint name="RL_thigh_joint" type="hinge" axis="0 1 0" range="-2.966 4.5" damping="0.1"/>
          <geom name="RL_thigh" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.041" rgba="0.8 0.8 0.3 1" mass="1.013"/>
          <inertial pos="0 0 -0.125" mass="1.013" diaginertia="0.005529065 0.005529065 0.000681723"/>
          
          <!-- Rear Left Calf -->
          <body name="RL_calf" pos="0 0 -0.25">
            <joint name="RL_calf_joint" type="hinge" axis="0 1 0" range="-2.77507 -0.645772" damping="0.1"/>
            <geom name="RL_calf" type="capsule" fromto="0 0 0 0 0 -0.25" size="0.02" rgba="0.8 0.8 0.3 1" mass="0.226"/>
            <inertial pos="0 0 -0.125" mass="0.226" diaginertia="0.001367788 0.001367788 0.000018050"/>
            
            <!-- Rear Left Foot -->
            <body name="RL_foot" pos="0 0 -0.25">
              <geom name="RL_foot" type="sphere" size="0.02" rgba="0.5 0.5 0.5 1" mass="0.06"/>
              <inertial pos="0 0 0" mass="0.06" diaginertia="0.000024 0.000024 0.000024"/>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>
  
  <!-- Actuators -->
  <actuator>
    <!-- Hip joints -->
    <motor name="FR_hip_motor" joint="FR_hip_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="FL_hip_motor" joint="FL_hip_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RR_hip_motor" joint="RR_hip_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RL_hip_motor" joint="RL_hip_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    
    <!-- Thigh joints -->
    <motor name="FR_thigh_motor" joint="FR_thigh_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="FL_thigh_motor" joint="FL_thigh_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RR_thigh_motor" joint="RR_thigh_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RL_thigh_motor" joint="RL_thigh_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    
    <!-- Calf joints -->
    <motor name="FR_calf_motor" joint="FR_calf_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="FL_calf_motor" joint="FL_calf_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RR_calf_motor" joint="RR_calf_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
    <motor name="RL_calf_motor" joint="RL_calf_joint" gear="33.5" ctrllimited="true" ctrlrange="-33.5 33.5"/>
  </actuator>
  
  <!-- Sensors -->
  <sensor>
    <!-- Joint position sensors -->
    <jointpos name="FR_hip_pos" joint="FR_hip_joint"/>
    <jointpos name="FL_hip_pos" joint="FL_hip_joint"/>
    <jointpos name="RR_hip_pos" joint="RR_hip_joint"/>
    <jointpos name="RL_hip_pos" joint="RL_hip_joint"/>
    <jointpos name="FR_thigh_pos" joint="FR_thigh_joint"/>
    <jointpos name="FL_thigh_pos" joint="FL_thigh_joint"/>
    <jointpos name="RR_thigh_pos" joint="RR_thigh_joint"/>
    <jointpos name="RL_thigh_pos" joint="RL_thigh_joint"/>
    <jointpos name="FR_calf_pos" joint="FR_calf_joint"/>
    <jointpos name="FL_calf_pos" joint="FL_calf_joint"/>
    <jointpos name="RR_calf_pos" joint="RR_calf_joint"/>
    <jointpos name="RL_calf_pos" joint="RL_calf_joint"/>
    
    <!-- Joint velocity sensors -->
    <jointvel name="FR_hip_vel" joint="FR_hip_joint"/>
    <jointvel name="FL_hip_vel" joint="FL_hip_joint"/>
    <jointvel name="RR_hip_vel" joint="RR_hip_joint"/>
    <jointvel name="RL_hip_vel" joint="RL_hip_joint"/>
    <jointvel name="FR_thigh_vel" joint="FR_thigh_joint"/>
    <jointvel name="FL_thigh_vel" joint="FL_thigh_joint"/>
    <jointvel name="RR_thigh_vel" joint="RR_thigh_joint"/>
    <jointvel name="RL_thigh_vel" joint="RL_thigh_joint"/>
    <jointvel name="FR_calf_vel" joint="FR_calf_joint"/>
    <jointvel name="FL_calf_vel" joint="FL_calf_joint"/>
    <jointvel name="RR_calf_vel" joint="RR_calf_joint"/>
    <jointvel name="RL_calf_vel" joint="RL_calf_joint"/>
    
    <!-- IMU sensor -->
    <accelerometer name="imu_accel" site="imu"/>
    <gyro name="imu_gyro" site="imu"/>
  </sensor>
</mujoco>
